<%= if logged_in?(@conn) do %>
  <li><%= link("Sign out", to: "/sessions/delete", method: "delete") %></li>

<% else %>
  <li><%= link "Create new accouunt", to: user_path(@conn, :new),
      class: "" %></li>
  <li><%= link "Sign in", to: session_path(@conn, :new),
      class: "" %></li>
<% end %>